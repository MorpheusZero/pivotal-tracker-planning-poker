{
  "name": "Pivotal Tracker Planning Poker Exporter",
  "version": "1.0.1",
  "description": "Allows exporting stories from Pivotal Tracker in the format that Planning Poker requires for Import.",
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://code.jquery.com; object-src 'self'",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "title": "PT-PP Exporter"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png"
  }
}
